<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xsupport.dao.measure.DisplacementGroupDao">

    <resultMap id="BaseResult" type="com.xsupport.model.measure.DisplacementGroup">
        <id column="id" property="id"/>
        <result column="createTime" property="createTime"/>
        <result column="remark" property="remark"/>

        <collection property="every"
                    column="id"
                    javaType="java.util.ArrayList"
                    ofType="com.xsupport.model.measure.DisplacementEvery"
                    select="com.xsupport.dao.measure.DisplacementEveryDao.findByGroupId">
        </collection>
    </resultMap>

    <select id="findNewestData" resultMap="BaseResult">
        SELECT * FROM displacement_group ORDER BY createTime DESC LIMIT 1
    </select>

</mapper>